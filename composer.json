{
  "name": "skillberto/git-bundle",
  "type": "symfony-bundle",
  "description": "GitBundle for the Symfony Framework",
  "keywords": ["git", "bundle"],
  "license": "MIT",
  "authors": [
    {
      "name": "Norbert Heiszler",
      "email": "heiszler.norbert@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/skillberto/gitter"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "symfony/twig-bridge": "~2.3|~3.0",
    "symfony/validator": "~2.3|~3.0",
    "klaussilveira/gitter": "dev-master"
  },
  "require-dev": {
    "symfony/config": "~2.3|~3.0",
    "symfony/dependency-injection": "~2.3|~3.0",
    "symfony/http-kernel": "~2.3|~3.0",
    "satooshi/php-coveralls": "~0.6.1",
    "phpunit/phpunit": "~4"
  },
  "autoload": {
    "psr-4": { "Skillberto\\GitBundle\\": "" }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}